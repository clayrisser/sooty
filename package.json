{
  "name": "sooty",
  "version": "0.0.13",
  "description": "A bit of oofle dust to scrape websites",
  "homepage": "https://github.com/jamrizzi/sooty",
  "author": {
    "name": "Jam Risser",
    "email": "jam@jamrizzi.com",
    "url": "https://jam.jamrizzi.com"
  },
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "build":
      "npm run clean && npm run test && npm run build:lib && npm run build:scripts",
    "build:lib": "babel src --ignore src/scripts -d lib",
    "build:scripts": "parcel build src/scripts/index.js -d lib -o scripts.js",
    "clean": "rm -rf lib .cache .tmp && jest --clearCache",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run build:scripts && jest --forceExit",
    "test:serve": "http-server tests/public"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "easy-joi": "0.0.2",
    "fs-extra": "^6.0.1",
    "idempotent-babel-polyfill": "^0.1.1",
    "isomorphic-dom": "0.0.2",
    "jest": "^22.4.3",
    "joi": "^13.1.2",
    "jsdom": "^11.10.0",
    "lodash": "^4.17.5",
    "murmurhash-native": "^3.2.3",
    "newregexp": "0.0.1",
    "puppeteer": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "detect-port": "^1.2.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "http-server": "^0.11.1",
    "parcel-bundler": "^1.8.1"
  },
  "license": "MIT",
  "repository": "https://github.com/jamrizzi/sooty",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ],
      "stage-2"
    ]
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "eslintIgnore": ["lib", "docs", ".cache", ".tmp"],
  "jest": {
    "testMatch": ["**/tests/**/*.js"],
    "verbose": true
  }
}
